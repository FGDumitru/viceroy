{
  "name": "fgdumitru/viceroy",
  "type": "library",
  "description": "Viceroy: An LLM interactions library with an OpenAI compatible completion endpoint via PHP.",
  "license": "MIT",
  "homepage": "https://github.com/FGDumitru/viceroy",
  "keywords": ["llm","openai","llama","llama.cpp","php"],
  "autoload": {
    "psr-4": {
      "Viceroy\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Florin Dumitru",
      "email": "xelotx@gmail.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.10",
    "league/html-to-markdown": "^5.1",
    "php": ">=8.1",
    "pixel418/markdownify": "^2.3",
    "league/commonmark": "^2.7",
    "ext-dom": "*"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
